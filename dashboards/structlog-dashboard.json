{
  "dashboard": {
    "id": null,
    "uid": "python-logging-loki",
    "title": "üêç Python Application Observability",
    "description": "Comprehensive monitoring dashboard for Python application with structured logging",
    "tags": ["python", "structlog", "loki", "observability"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "templating": {
      "list": [
        {
          "name": "container",
          "type": "query",
          "query": "label_values(container)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "üìä System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate({job=\"python_app_logs\"} | json [5m]))",
            "legendFormat": "Total Requests/sec",
            "refId": "A"
          },
          {
            "expr": "(sum(rate({job=\"python_app_logs\"} | json | level = \"ERROR\" [5m])) / sum(rate({job=\"python_app_logs\"} | json [5m]))) * 100",
            "legendFormat": "Error Rate %",
            "refId": "B"
          },
          {
            "expr": "avg(avg_over_time({job=\"python_app_logs\"} | json | unwrap response_time_ms [5m]))",
            "legendFormat": "Avg Response Time (ms)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "short",
            "decimals": 2
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Error Rate %"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 2},
                    {"color": "red", "value": 5}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Avg Response Time (ms)"},
              "properties": [
                {"id": "unit", "value": "ms"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 1000},
                    {"color": "red", "value": 2000}
                  ]
                }}
              ]
            }
          ]
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "üìà Request Volume Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (method) (rate({job=\"python_app_logs\"} | json | method != \"\" [1m]))",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 3,
        "title": "üö® Error Rate Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(sum(rate({job=\"python_app_logs\"} | json | level = \"ERROR\" [1m])) / sum(rate({job=\"python_app_logs\"} | json [1m]))) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          },
          {
            "expr": "(sum(rate({job=\"python_app_logs\"} | json | level = \"WARNING\" [1m])) / sum(rate({job=\"python_app_logs\"} | json [1m]))) * 100",
            "legendFormat": "Warning Rate %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 4,
        "title": "‚ö° Response Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate({job=\"python_app_logs\"} | json | unwrap response_time_ms [5m])) by (le))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate({job=\"python_app_logs\"} | json | unwrap response_time_ms [5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate({job=\"python_app_logs\"} | json | unwrap response_time_ms [5m])) by (le))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"drawStyle": "line", "lineWidth": 2},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14}
      },
      {
        "id": 5,
        "title": "üîó HTTP Status Codes",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status_code) (count_over_time({job=\"python_app_logs\"} | json | status_code != \"\" [5m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": []
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14}
      },
      {
        "id": 6,
        "title": "üë• Business Operations",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase({job=\"python_app_logs\"} | json | msg =~ \".*registration successful.*\" [5m]))",
            "legendFormat": "User Registrations",
            "refId": "A"
          },
          {
            "expr": "sum(increase({job=\"python_app_logs\"} | json | msg =~ \".*order.*successful.*\" [5m]))",
            "legendFormat": "Successful Orders",
            "refId": "B"
          },
          {
            "expr": "sum(increase({job=\"python_app_logs\"} | json | msg =~ \".*authentication successful.*\" [5m]))",
            "legendFormat": "Successful Logins",
            "refId": "C"
          },
          {
            "expr": "sum(increase({job=\"python_app_logs\"} | json | msg =~ \".*upload completed.*\" [5m]))",
            "legendFormat": "File Uploads",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 22}
      },
      {
        "id": 7,
        "title": "üîç Top API Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (path) (count_over_time({job=\"python_app_logs\"} | json | path != \"\" [5m])))",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 8,
        "title": "üö® Recent Errors",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"python_app_logs\"} | json | level = \"ERROR\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "showLabels": false,
              "showTime": true,
              "sortOrder": "Descending",
              "wrapLogMessage": true
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      },
      {
        "id": 9,
        "title": "üîí Security Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate({job=\"python_app_logs\"} | json | msg =~ \".*authentication.*\" | level = \"WARNING\" [1m]))",
            "legendFormat": "Failed Auth Attempts",
            "refId": "A"
          },
          {
            "expr": "sum(rate({job=\"python_app_logs\"} | json | security_alert = \"true\" [1m]))",
            "legendFormat": "Security Alerts",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"drawStyle": "line", "lineWidth": 2},
            "unit": "cps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36}
      },
      {
        "id": 10,
        "title": "üíæ System Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(avg_over_time({job=\"python_app_logs\"} | json | msg =~ \".*analytics.*\" | unwrap processing_time_ms [5m]))",
            "legendFormat": "Analytics Query Time",
            "refId": "A"
          },
          {
            "expr": "avg(avg_over_time({job=\"python_app_logs\"} | json | msg =~ \".*upload.*\" | unwrap upload_time_sec * 1000 [5m]))",
            "legendFormat": "File Upload Time",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"drawStyle": "line", "lineWidth": 2},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36}
      },
      {
        "id": 11,
        "title": "üìã Live Log Stream",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"python_app_logs\"} | json",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "showLabels": false,
              "showTime": true,
              "sortOrder": "Descending",
              "wrapLogMessage": true,
              "deduplication": false
            }
          }
        },
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 44}
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Loki",
          "enable": true,
          "expr": "{job=\"python_app_logs\"} | json | msg =~ \".*Starting logging application.*\"",
          "textFormat": "App Started",
          "titleFormat": "Deployment"
        }
      ]
    }
  }
}